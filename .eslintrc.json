{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "settings": {
    "import/resolver": "node",
    "import/extensions": [".js", ".jsx", ".ts", ".tsx"]
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "quotes": ["error", "single"]
  }
}
